{
    "name": "n3xt0r/zf2-otrs ",
    "description" : "Integrates OTRS 4 in ZF2",
    "require" : {
        "php" : ">=5.4.0",
        "zendframework/zendframework" : ">=2.3.2,<3.0.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/N3XT0R/ZF2-OTRS"
        }
    ],
    "authors" : [ 
        {
            "name" : "Ilya Beliaev",
            "email" : "info@php-dev.info",
            "homepage" : "http://blog.php-dev.info"
        } 
    ],
    "autoload" : {
        "psr-4" : {
            "OTRS\\" : "src/"
        },
        "psr-0" : {
            "OTRS\\" : "src/"
        },
        "classmap" : [ "./" ]
    },
    "keywords" : [ "N3XT0R", "OTRS", "module", "zf2" ],
    "type" : "library",
    "license" : "GPL-3.0",
    "homepage" : "https://github.com/N3XT0R/ZF2-OTRS"
}